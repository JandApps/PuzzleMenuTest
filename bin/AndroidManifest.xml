<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.puzzlemenutest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/puzzle"
        android:label="@string/app_name" >
        <activity
            android:name="com.example.puzzlemenutest.menus.MainMenuActivity"
            android:theme="@style/AppThemeCustomFont" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.puzzlemenutest.menus.NewGameMenuActivity"
            android:theme="@style/AppThemeCustomFont" >
        </activity>
        <activity
            android:name="com.example.puzzlemenutest.GameActivity"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        </activity>
    </application>

</manifest>